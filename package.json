{
    "name": "aerian-site-rebuild",
    "description": "Aerian Studios LLC.",
    "version": "0.0.1",
    "author": "sunya.dickman@aerian.com",
    "dependencies": {
        "gatsby": "^1.9.272",
        "gatsby-image": "^1.0.54",
        "gatsby-link": "^1.6.40",
        "gatsby-plugin-manifest": "^1.0.27",
        "gatsby-plugin-netlify-cms": "^2.0.1",
        "gatsby-plugin-offline": "^1.0.18",
        "gatsby-plugin-react-helmet": "^2.0.11",
        "gatsby-plugin-react-next": "^1.0.11",
        "gatsby-plugin-sass": "^1.0.26",
        "gatsby-plugin-sharp": "^1.6.48",
        "gatsby-plugin-typescript": "^1.4.20",
        "gatsby-source-filesystem": "^1.5.39",
        "gatsby-transformer-json": "^1.0.20",
        "gatsby-transformer-sharp": "^1.6.27",
        "netlify-cms": "^1.9.2",
        "react-helmet": "^5.2.0"
    },
    "keywords": [
        "gatsby"
    ],
    "license": "MIT",
    "scripts": {
        "build": "gatsby build",
        "start": "gatsby develop",
        "test": "jest",
        "updateSnapshot": "jest --updateSnapshot",
        "storybook": "start-storybook -p 9001 -c .storybook",
        "precommit": "lint-staged",
        "prepush": "jest --ci"
    },
    "devDependencies": {
        "@storybook/addon-actions": "^3.4.8",
        "@storybook/addon-info": "^3.4.8",
        "@storybook/react": "^3.4.8",
        "@types/jest": "^23.1.1",
        "@types/react-test-renderer": "^16.0.1",
        "@types/storybook__addon-actions": "^3.0.3",
        "@types/storybook__addon-info": "^3.2.3",
        "@types/storybook__react": "^3.0.7",
        "gh-pages": "^1.2.0",
        "husky": "^0.14.3",
        "hygen": "^1.6.2",
        "hygen-react-typescript": "^1.0.2",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^23.1.0",
        "lint-staged": "^7.2.0",
        "prettier": "^1.13.5",
        "react-docgen-typescript-loader": "^2.1.1",
        "react-docgen-typescript-webpack-plugin": "^1.1.0",
        "react-test-renderer": "^15.6.2",
        "ts-jest": "^22.4.6",
        "tslint": "^5.10.0",
        "tslint-config-aerian": "^1.0.2",
        "typescript": "^2.9.2",
        "typings-for-css-modules-loader": "^1.7.0",
        "webpack": "^3.12.0"
    },
    "lint-staged": {
        "*.{js,jsx,css,md,scss}": [
            "prettier --write",
            "git add",
            "jest --ci --findRelatedTests"
        ],
        "*.{ts,tsx}": [
            "tslint --fix",
            "git add",
            "jest --ci --findRelatedTests"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com:aerian-studios/aerian-site-rebuild.git"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ],
        "moduleNameMapper": {
            "^.+\\.(css|less|scss)$": "identity-obj-proxy"
        }
    }
}
