{
    "name": "aerian-site-rebuild",
    "description": "Aerian Studios LLC.",
    "version": "0.0.1",
    "author": "sunya.dickman@aerian.com",
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.12",
        "@fortawesome/free-brands-svg-icons": "^5.6.3",
        "@fortawesome/free-solid-svg-icons": "^5.6.3",
        "@fortawesome/react-fontawesome": "^0.1.3",
        "@researchgate/react-intersection-observer": "^0.7.4",
        "@types/react-transition-group": "^2.0.15",
        "classnames": "^2.2.6",
        "deep-map": "^1.5.0",
        "extract-text-webpack-plugin": "^3.0.2",
        "gatsby": "^2.1.24",
        "gatsby-image": "^2.0.26",
        "gatsby-plugin-manifest": "^2.0.22",
        "gatsby-plugin-netlify": "^2.0.6",
        "gatsby-plugin-netlify-cache": "^1.0.0",
        "gatsby-plugin-netlify-cms": "^3.0.9",
        "gatsby-plugin-offline": "^2.0.21",
        "gatsby-plugin-react-helmet": "^3.0.8",
        "gatsby-plugin-sass": "^2.0.7",
        "gatsby-plugin-sharp": "^2.0.25",
        "gatsby-plugin-typescript": "^2.0.3",
        "gatsby-source-filesystem": "^2.0.16",
        "gatsby-transformer-json": "^2.1.7",
        "gatsby-transformer-sharp": "2.1.15",
        "netlify-cms": "^2.6.0",
        "pigeon-maps": "^0.11.10",
        "pigeon-overlay": "^0.2.2",
        "react": "^16.8.4",
        "react-dom": "^16.8.4",
        "react-helmet": "^5.2.0",
        "react-markdown": "^4.0.6",
        "react-transition-group": "^2.5.2"
    },
    "keywords": [
        "gatsby"
    ],
    "license": "MIT",
    "scripts": {
        "build": "gatsby build",
        "develop": "gatsby develop",
        "start": "run-p develop",
        "test": "jest",
        "test-watch": "jest --watchAll --coverage",
        "updateSnapshot": "jest --updateSnapshot",
        "storybook": "start-storybook -p 9001 -c .storybook",
        "storybook-build": "build-storybook -c .storybook -o docs",
        "test:circle": "jest --ci --runInBand --verbose --silent",
        "test-ci": "jest --coverage --coverageReporters=text-lcov --maxWorkers=2 > coverage.lcov && codecov"
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.2.3",
        "@babel/plugin-proposal-optional-chaining": "^7.2.0",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/preset-env": "^7.2.3",
        "@babel/preset-react": "^7.0.0",
        "@storybook/addon-actions": "^4.1.6",
        "@storybook/addon-info": "^4.1.6",
        "@storybook/react": "^4.1.6",
        "@types/classnames": "^2.2.7",
        "@types/jest": "^24.0.9",
        "@types/node": "^11.10.5",
        "@types/react-dom": "^16.0.11",
        "@types/react-helmet": "^5.0.8",
        "@types/react-test-renderer": "^16.0.3",
        "@types/storybook__addon-actions": "^3.4.1",
        "@types/storybook__addon-info": "^3.4.2",
        "@types/storybook__react": "^4.0.0",
        "autoprefixer": "^9.4.10",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^24.3.1",
        "babel-loader": "^8.0.5",
        "babel-plugin-require-context-hook": "^1.0.0",
        "codecov": "^3.1.0",
        "css-loader": "^2.1.1",
        "extract-text-webpack-plugin": "^3.0.2",
        "gh-pages": "^2.0.1",
        "husky": "^1.3.1",
        "hygen": "^3.0.0",
        "hygen-react-typescript": "^1.0.2",
        "identity-obj-proxy": "^3.0.0",
        "intersection-observer": "^0.5.1",
        "jest": "^24.3.1",
        "lint-staged": "^8.1.5",
        "node-sass": "^4.11.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.15.3",
        "react-docgen-typescript-loader": "^3.0.1",
        "react-docgen-typescript-webpack-plugin": "^1.1.0",
        "react-test-renderer": "^16.8.4",
        "regenerator-runtime": "^0.13.1",
        "ts-jest": "^24.0.0",
        "ts-loader": "5.3.3",
        "tslint": "^5.12.1",
        "tslint-config-aerian": "^1.0.3",
        "typescript": "^3.2.2",
        "typings-for-css-modules-loader": "^1.7.0"
    },
    "lint-staged": {
        "*.{js,jsx,css,md,scss}": [
            "prettier --write",
            "git add",
            "jest --ci --findRelatedTests"
        ],
        "*.{ts,tsx}": [
            "tslint --fix",
            "git add",
            "jest --ci --findRelatedTests"
        ]
    },
    "husky": {
        "hooks": {
            "precommit": "lint-staged",
            "prepush": "jest --ci"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com:aerian-studios/aerian-site-rebuild.git"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest",
            "^.+\\.jsx?$": "<rootDir>/jestPreprocess.js"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ],
        "moduleNameMapper": {
            "^.+\\.(css|less|scss)$": "identity-obj-proxy",
            ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts"
        },
        "transformIgnorePatterns": [
            "node_modules/(?!(gatsby)/)"
        ],
        "setupFiles": [
            "<rootDir>/loadershim.js"
        ],
        "globals": {
            "__PATH_PREFIX__": ""
        }
    }
}
