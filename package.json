{
    "name": "aerian-site-rebuild",
    "description": "Aerian Studios Ltd.",
    "version": "0.0.1",
    "author": "sunya.dickman@aerian.com",
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.12",
        "@fortawesome/free-brands-svg-icons": "^5.6.3",
        "@fortawesome/free-solid-svg-icons": "^5.6.3",
        "@fortawesome/react-fontawesome": "^0.1.3",
        "@researchgate/react-intersection-observer": "^0.7.4",
        "@types/react-transition-group": "^2.0.15",
        "classnames": "^2.2.6",
        "deep-map": "^1.5.0",
        "dotenv": "^8.1.0",
        "extract-text-webpack-plugin": "^3.0.2",
        "gatsby": "^2.13.27",
        "gatsby-image": "^2.2.6",
        "gatsby-plugin-manifest": "^2.2.3",
        "gatsby-plugin-netlify": "^2.1.3",
        "gatsby-plugin-netlify-cache": "^1.2.0",
        "gatsby-plugin-netlify-cms": "^4.1.6",
        "gatsby-plugin-offline": "^2.2.4",
        "gatsby-plugin-react-helmet": "^3.1.2",
        "gatsby-plugin-sass": "^2.1.3",
        "gatsby-plugin-sharp": "2.2.19",
        "gatsby-plugin-typescript": "^2.1.2",
        "gatsby-source-filesystem": "^2.1.5",
        "gatsby-source-twitter": "^3.0.0",
        "gatsby-transformer-json": "^2.2.2",
        "gatsby-transformer-sharp": "^2.2.4",
        "netlify-cms": "^2.9.6",
        "pigeon-maps": "^0.10.4",
        "pigeon-overlay": "^0.2.2",
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "react-helmet": "^5.2.0",
        "react-markdown": "^3.6.0",
        "react-transition-group": "^2.5.2"
    },
    "keywords": [
        "gatsby"
    ],
    "license": "MIT",
    "scripts": {
        "build": "gatsby build",
        "develop": "gatsby develop",
        "start": "run-p develop",
        "test": "jest",
        "test-watch": "jest --watchAll --coverage",
        "updateSnapshot": "jest --updateSnapshot",
        "storybook": "NODE_ENV=production start-storybook -p 9001 -c .storybook -s public",
        "storybook-build": "NODE_ENV=production build-storybook -c .storybook -o docs -s public",
        "precommit": "lint-staged",
        "lint": "eslint --ext=ts,tsx  .",
        "prepush": "jest --ci",
        "test:circle": "jest --ci --runInBand --verbose --silent",
        "test-ci": "jest --coverage --coverageReporters=text-lcov --maxWorkers=2 > coverage.lcov && codecov"
    },
    "devDependencies": {
        "@aerian/eslint-config": "^2.0.1",
        "@aerian/eslint-config-react": "^2.0.1",
        "@aerian/eslint-config-typescript": "2.0.2",
        "@babel/core": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.2.3",
        "@babel/plugin-proposal-optional-chaining": "^7.2.0",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/plugin-transform-runtime": "^7.5.5",
        "@babel/preset-env": "^7.2.3",
        "@babel/preset-react": "^7.0.0",
        "@storybook/addon-actions": "^5.1.11",
        "@storybook/addon-info": "^5.1.11",
        "@storybook/addon-storyshots": "^5.1.11",
        "@storybook/react": "^5.1.11",
        "@testing-library/jest-dom": "^4.1.0",
        "@testing-library/react": "^9.1.3",
        "@types/classnames": "^2.2.7",
        "@types/jest": "^23.3.12",
        "@types/node": "^12.7.3",
        "@types/react-dom": "^16.0.11",
        "@types/react-helmet": "^5.0.8",
        "@types/react-test-renderer": "^16.0.3",
        "@types/storybook__addon-actions": "^3.4.3",
        "@types/storybook__addon-info": "^4.1.2",
        "@types/storybook__addon-storyshots": "^5.1.1",
        "@types/storybook__react": "^4.0.2",
        "autoprefixer": "^8.6.5",
        "babel-core": "^7.0.0-bridge.0",
        "babel-jest": "^24.9.0",
        "babel-loader": "^8.0.5",
        "babel-plugin-remove-graphql-queries": "^2.7.6",
        "babel-plugin-require-context-hook": "^1.0.0",
        "babel-preset-gatsby": "^0.2.12",
        "codecov": "^3.5.0",
        "css-loader": "^0.28.11",
        "eslint": "^6.0.1",
        "eslint-plugin-import": "^2.18.0",
        "eslint-plugin-prettier": "^3.1.0",
        "extract-text-webpack-plugin": "^3.0.2",
        "gh-pages": "^1.2.0",
        "husky": "^0.14.3",
        "hygen": "^1.6.2",
        "hygen-react-typescript": "^1.0.2",
        "identity-obj-proxy": "^3.0.0",
        "intersection-observer": "^0.5.1",
        "jest": "^24.9.0",
        "lint-staged": "^7.3.0",
        "node-sass": "^4.11.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.15.3",
        "react-docgen-typescript-loader": "^2.2.0",
        "react-docgen-typescript-webpack-plugin": "^1.1.0",
        "react-test-renderer": "^16.7.0",
        "regenerator-runtime": "^0.12.1",
        "ts-jest": "^24.0.2",
        "ts-loader": "4",
        "typescript": "^3.2.2",
        "typings-for-css-modules-loader": "^1.7.0"
    },
    "eslintConfig": {
        "extends": [
            "@aerian/eslint-config-react",
            "@aerian/eslint-config-typescript"
        ],
        "rules": {
            "jsx-a11y/heading-has-content": 0
        }
    },
    "lint-staged": {
        "*.{css,md,scss}": [
            "prettier --write",
            "git add",
            "jest --ci --findRelatedTests"
        ],
        "*.{ts,tsx,js,jsx}": [
            "eslint --fix",
            "git add",
            "jest --ci --findRelatedTests"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com:aerian-studios/aerian-site-rebuild.git"
    },
    "jest": {
        "transform": {
            "^.+\\.[jt]sx?$": "<rootDir>/jestPreprocess.js"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
        "moduleNameMapper": {
            "^.+\\.(css|less|scss)$": "identity-obj-proxy",
            ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts"
        },
        "transformIgnorePatterns": [
            "node_modules/(?!(gatsby)/)"
        ],
        "setupFiles": [
            "<rootDir>/loadershim.js"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/jest.setup.js"
        ],
        "globals": {
            "__PATH_PREFIX__": "",
            "ts-jest": {
                "diagnostics": false
            }
        }
    },
    "resolutions": {
        "sharp": "0.22.1"
    }
}
